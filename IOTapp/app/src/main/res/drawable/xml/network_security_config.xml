<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system" />   //表示信任系统的CA证书
            <certificates src="user" /> // 信任用户安装证书
        </trust-anchors>
    </base-config>
</network-security-config>